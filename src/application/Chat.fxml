<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane maxHeight="600.0" maxWidth="600.0" minHeight="600.0" minWidth="600.0" prefHeight="600.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.ChatController">
   <bottom>
      <HBox prefHeight="170.0" prefWidth="600.0" BorderPane.alignment="CENTER">
         <children>
            <TextArea fx:id="txtMessage" prefHeight="170.0" prefWidth="506.0" wrapText="true" onKeyPressed="#sendEnter" />
            <Button fx:id="btSend" defaultButton="true" mnemonicParsing="false" onAction="#sendPressed" prefHeight="169.0" prefWidth="94.0" text="Send" />
         </children>
      </HBox>
   </bottom>
   <center>
      <TextArea fx:id="txtMessages" editable="false" prefHeight="406.0" prefWidth="600.0" BorderPane.alignment="CENTER">
         <BorderPane.margin>
            <Insets bottom="5.0" />
         </BorderPane.margin></TextArea>
   </center>
   <padding>
      <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
   </padding>
</BorderPane>